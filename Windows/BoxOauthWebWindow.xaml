<Window x:Class="BoxNestGroup.BoxOauthWebWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BoxNestGroup"
        xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        mc:Ignorable="d"
        Title="BoxOAuthWebWindow" Height="768" Width="1024"
        Closing="windowClosing">
    <Grid>
        <!---
        <WebBrowser Name="BoxOAuthWebBrowser" Navigated="boxOAuthWebBrowserNavigated"/>
        -->
        <wv2:WebView2 Name="BoxOAuthWebBrowser" NavigationCompleted="boxOAuthWebBrowserNavigationCompleted" />
    </Grid>
</Window>
